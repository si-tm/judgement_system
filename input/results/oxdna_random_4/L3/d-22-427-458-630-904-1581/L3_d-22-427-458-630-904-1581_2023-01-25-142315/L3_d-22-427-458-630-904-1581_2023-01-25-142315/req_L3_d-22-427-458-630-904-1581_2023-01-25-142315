# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c c d @initial 1.0 M
s1 = a a* f @initial 1.0 M
s2 = f f* e* @initial 1.0 M
s3 = d b* c @initial 1.0 M
s4 = b f* d* @initial 1.0 M
s5 = b* b b @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CATGTAGTCAACGCATT
domains b = TATGTTGAAGTCGTTTA
domains c = TGACTAAGAACCTGTGA
domains d = GAGACCCTGTGTCGG
domains e = ATTAATTGAGGGCCACA
domains f = GTCGGTCAAGTAGCCCC
