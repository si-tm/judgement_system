# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = d a a* @initial 1.0 M
s1 = f c* e @initial 1.0 M
s2 = f e* b* @initial 1.0 M
s3 = d* b d @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GCGAGATCGTCAGGATG
domains b = GTAAGTTGCAATTCCGG
