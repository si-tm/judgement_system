# type_of_l
type_of_l = L2

# structure
number_of_types = 6
s0 = b* b a* b* @initial 1.0 M
s1 = b a a b @initial 1.0 M
s2 = a b* a b* @initial 1.0 M
s3 = a b b* a* @initial 1.0 M
s4 = a* b* a b* @initial 1.0 M
s5 = b b* b a @initial 1.0 M

# length
length a = 5
length b = 5

# sequence of domains
domains a = TGTAT
domains b = GATAT
