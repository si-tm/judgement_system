# type_of_l
type_of_l = L2

# structure
number_of_types = 5
s0 = b* a b b* @initial 1.0 M
s1 = a b b a* @initial 1.0 M
s2 = b* b b a @initial 1.0 M
s3 = b b a* b* @initial 1.0 M
s4 = a a* a* b @initial 1.0 M

# length
length a = 5
length b = 5

# sequence of domains
domains a = GCCGA
domains b = GAATA
