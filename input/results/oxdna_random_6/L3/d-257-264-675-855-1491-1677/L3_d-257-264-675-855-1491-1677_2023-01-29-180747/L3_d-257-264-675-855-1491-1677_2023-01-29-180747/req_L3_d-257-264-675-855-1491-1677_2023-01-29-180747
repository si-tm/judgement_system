# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c e b* @initial 1.0 M
s1 = f* d* e* @initial 1.0 M
s2 = a* e* c* @initial 1.0 M
s3 = c* f* b* @initial 1.0 M
s4 = a* f a @initial 1.0 M
s5 = f c b* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GAGAGAGGTGGACATTA
domains b = GTAAGGTATGCGAAACA
