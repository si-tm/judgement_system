# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a a f @initial 1.0 M
s1 = b a* f @initial 1.0 M
s2 = b c* a* @initial 1.0 M
s3 = d d* c* @initial 1.0 M
s4 = d f* d* @initial 1.0 M
s5 = c b* c @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GCAGGAAACTAATACAG
domains b = CAATTCCGATAGTAGCT
domains c = AGTCGGATGGATTTAGC
domains d = CTGTACACGCGGCGG
