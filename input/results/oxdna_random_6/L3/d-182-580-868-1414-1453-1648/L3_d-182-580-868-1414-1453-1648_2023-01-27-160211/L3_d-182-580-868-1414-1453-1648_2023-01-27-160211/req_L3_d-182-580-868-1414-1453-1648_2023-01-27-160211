# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = f* c* c @initial 1.0 M
s1 = e* e* f @initial 1.0 M
s2 = a* b* b @initial 1.0 M
s3 = d a c @initial 1.0 M
s4 = f a* a* @initial 1.0 M
s5 = c a c @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

