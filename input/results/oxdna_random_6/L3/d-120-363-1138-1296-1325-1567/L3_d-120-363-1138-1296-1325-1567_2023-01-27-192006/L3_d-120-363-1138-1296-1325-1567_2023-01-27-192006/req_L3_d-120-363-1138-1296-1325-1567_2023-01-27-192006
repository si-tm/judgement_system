# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a f a @initial 1.0 M
s1 = d* f f @initial 1.0 M
s2 = b d b* @initial 1.0 M
s3 = e* a a @initial 1.0 M
s4 = f f d* @initial 1.0 M
s5 = e* b c* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = AGGAGAGCGGTGATAAG
domains b = CGTAACCCAAAATGGAT
domains c = CTTCTTTTGCTAATTTC
domains d = TGTAGGTGTCTTTAC
