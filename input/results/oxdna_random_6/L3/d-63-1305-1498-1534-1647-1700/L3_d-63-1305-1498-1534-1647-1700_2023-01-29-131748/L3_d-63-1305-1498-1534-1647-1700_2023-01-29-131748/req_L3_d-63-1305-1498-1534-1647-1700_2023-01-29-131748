# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = f c f @initial 1.0 M
s1 = f* c* b* @initial 1.0 M
s2 = f* e* e @initial 1.0 M
s3 = a c* b* @initial 1.0 M
s4 = f d* f @initial 1.0 M
s5 = e* a e* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

