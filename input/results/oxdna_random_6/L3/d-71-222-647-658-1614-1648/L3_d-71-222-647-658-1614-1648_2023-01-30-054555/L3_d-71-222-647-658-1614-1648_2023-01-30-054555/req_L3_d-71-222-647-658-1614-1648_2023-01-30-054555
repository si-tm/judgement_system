# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a c* f* @initial 1.0 M
s1 = f* c* c @initial 1.0 M
s2 = c c* f* @initial 1.0 M
s3 = c d f @initial 1.0 M
s4 = f* b d @initial 1.0 M
s5 = a* d d @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GTAATTCGCATGTAAGC
domains b = CACGGGCGGCAACATTC
domains c = TCAAAGATTCGAAAATA
