# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = b* c b* @initial 1.0 M
s1 = a* a a* @initial 1.0 M
s2 = d* a d* @initial 1.0 M
s3 = b* f* e @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CGTCGCAGTTCGGGCCT
domains b = ATGTTTACAACGGAATC
domains c = GACATTGATGCCCCGCG
domains d = CGCGTCGCCTGACAG
domains e = TGAATTCGGAAAGTTAG
domains f = CTGGTGTAACCGTTGAT
