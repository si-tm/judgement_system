# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = f c a @initial 1.0 M
s1 = b* c* a @initial 1.0 M
s2 = f* c f @initial 1.0 M
s3 = f* b e* @initial 1.0 M

# length
length a = 17
