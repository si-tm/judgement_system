# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a* c c @initial 1.0 M
s1 = a* f c* @initial 1.0 M
s2 = a d* b @initial 1.0 M
s3 = f a* f @initial 1.0 M
s4 = d d c @initial 1.0 M
s5 = d* b b @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CTTTGATGGCTTAGACC
domains b = GGTTAATGTTTTAAGCC
domains c = TAATGAGGTCGGCGTGC
domains d = ACTGTCAGATTGTCA
