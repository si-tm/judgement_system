length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = ATGTTCCGTCCGACTGC
domains b = CTTTCCCGAGGTGTGCA
domains c = GTCTCTGAAACCCCCAG
domains d = CTCGACGGAATAGAG
