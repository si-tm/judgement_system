# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c* b* c @initial 1.0 M
s1 = a c* e @initial 1.0 M
s2 = c* b a* @initial 1.0 M
s3 = e* f* a @initial 1.0 M
s4 = a* e* e* @initial 1.0 M
s5 = f* e* a @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = TGTAGAGAGAACGCCAA
domains b = GCTTGCTGGGATGACAT
domains c = GCATCTTGTGTAAAATA
