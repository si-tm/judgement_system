# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = a* a f @initial 1.0 M
s1 = c* d* f @initial 1.0 M
s2 = c* a d @initial 1.0 M
s3 = c* c e @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GTTCTATAAGCCATAAT
domains b = TGCCTAGACTACTTCCT
domains c = AAACCCCATTAAACAAT
domains d = ACTTCCGACAAGCAC
domains e = CAAGGTGCGCATACCTC
domains f = ATAGAGTTATTCTTCAT
