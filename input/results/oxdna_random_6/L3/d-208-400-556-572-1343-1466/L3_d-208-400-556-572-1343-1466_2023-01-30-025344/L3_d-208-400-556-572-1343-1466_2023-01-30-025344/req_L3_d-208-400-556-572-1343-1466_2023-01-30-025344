# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = b e* c @initial 1.0 M
s1 = b* f c @initial 1.0 M
s2 = a* c* c @initial 1.0 M
s3 = b* f* e @initial 1.0 M
s4 = f b b @initial 1.0 M
s5 = e* b* f* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GGTTCGAGTACTACAGG
domains b = TCACCCGCGTTATGGGC
domains c = GCTGGACCCCCTTAGGT
domains d = CGTAGGCAACAGAAA
domains e = CTAAATTAGTACTGTGA
