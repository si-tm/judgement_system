# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c d f* @initial 1.0 M
s1 = a a e @initial 1.0 M
s2 = c* d* a @initial 1.0 M
s3 = c a* f* @initial 1.0 M
s4 = f b* e @initial 1.0 M
s5 = f f d* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CAGCGTACTTCACTTTA
domains b = GATCCCTTAAACAAGGT
domains c = CGGTCCCATCTGTGTAA
domains d = CCCGCCTCGCAGTAC
domains e = GAGAACAGAGCTCCTGT
