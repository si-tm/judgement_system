# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = f d* e* @initial 1.0 M
s1 = e* d* b @initial 1.0 M
s2 = d* d* c @initial 1.0 M
s3 = d f a* @initial 1.0 M
s4 = f* e* a* @initial 1.0 M
s5 = c f* e @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

