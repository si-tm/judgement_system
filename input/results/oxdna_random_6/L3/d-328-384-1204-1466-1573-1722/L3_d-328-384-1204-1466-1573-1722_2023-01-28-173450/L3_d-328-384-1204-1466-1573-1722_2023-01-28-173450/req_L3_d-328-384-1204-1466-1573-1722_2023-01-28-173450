# structure
number_of_types = 6
s0 = b b* c @initial 1.0 M
s1 = e c c @initial 1.0 M
s2 = f b b @initial 1.0 M
s3 = b e a @initial 1.0 M
s4 = f f* a* @initial 1.0 M
s5 = f* f* d @initial 1.0 M

# length
length a = 17
