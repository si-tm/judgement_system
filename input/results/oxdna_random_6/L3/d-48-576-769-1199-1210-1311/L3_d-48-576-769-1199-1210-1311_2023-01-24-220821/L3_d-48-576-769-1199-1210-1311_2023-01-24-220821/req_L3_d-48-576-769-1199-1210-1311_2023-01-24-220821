# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a c a @initial 1.0 M
s1 = c a a @initial 1.0 M
s2 = c* c a* @initial 1.0 M
s3 = e c f @initial 1.0 M
s4 = e* a* b* @initial 1.0 M
s5 = e b* f* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = TTTAAGCTTCGAGTCAG
domains b = ACTCAGCCTTCTGACGA
domains c = TTACCGCTTGACCTGTA
domains d = TTTACTTCTAATGGC
