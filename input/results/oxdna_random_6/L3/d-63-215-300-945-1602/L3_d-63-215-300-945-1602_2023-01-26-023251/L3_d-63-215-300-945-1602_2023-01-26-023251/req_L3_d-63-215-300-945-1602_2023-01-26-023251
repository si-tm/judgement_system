# type_of_l
type_of_l = L3

# structure
number_of_types = 5
s0 = a* c* f* @initial 1.0 M
s1 = a c* b* @initial 1.0 M
s2 = b a* a @initial 1.0 M
s3 = d d e* @initial 1.0 M
s4 = f* a* d @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CCATAGGACCAGGTACG
domains b = AGCAGACCACGATGGCC
domains c = TTCAGCGAGCACTTGAA
domains d = TCTCCTACGCAACAG
domains e = AAGTTGAAATATACCAG
