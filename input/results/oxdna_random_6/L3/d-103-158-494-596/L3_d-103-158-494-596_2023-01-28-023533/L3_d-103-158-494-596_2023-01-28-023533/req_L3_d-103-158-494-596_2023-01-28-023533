# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = a e d* @initial 1.0 M
s1 = a* a* b @initial 1.0 M
s2 = b* c* b @initial 1.0 M
s3 = c a* e @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

