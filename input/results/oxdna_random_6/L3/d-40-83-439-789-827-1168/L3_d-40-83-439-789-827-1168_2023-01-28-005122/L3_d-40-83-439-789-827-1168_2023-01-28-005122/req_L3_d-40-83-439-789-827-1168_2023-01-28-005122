# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a b* c @initial 1.0 M
s1 = a d f* @initial 1.0 M
s2 = c* e f* @initial 1.0 M
s3 = e a* c @initial 1.0 M
s4 = b* a d* @initial 1.0 M
s5 = c* c* e* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

