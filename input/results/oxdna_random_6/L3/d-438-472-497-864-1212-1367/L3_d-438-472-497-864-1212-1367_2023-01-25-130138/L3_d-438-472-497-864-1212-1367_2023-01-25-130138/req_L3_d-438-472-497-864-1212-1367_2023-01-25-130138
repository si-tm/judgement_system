# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = b* b* c @initial 1.0 M
s1 = b* c* c* @initial 1.0 M
s2 = e* c* f* @initial 1.0 M
s3 = d a a @initial 1.0 M
s4 = e c* a @initial 1.0 M
s5 = b* a d @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

