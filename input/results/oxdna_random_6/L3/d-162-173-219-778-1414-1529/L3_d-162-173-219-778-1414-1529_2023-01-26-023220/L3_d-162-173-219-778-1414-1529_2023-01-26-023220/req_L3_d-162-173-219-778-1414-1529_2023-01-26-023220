# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c* c f @initial 1.0 M
s1 = f d* c* @initial 1.0 M
s2 = e* e* f @initial 1.0 M
s3 = a* a* d @initial 1.0 M
s4 = a* d b* @initial 1.0 M
s5 = a* b c* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CGCTTAACCCATACGCT
domains b = GTCACTCTATTACGTAG
domains c = ATCAGCACAGAGCCTGG
domains d = GCCTCATATTGATGG
domains e = GCAGATCGACCGTTAGT
domains f = TAGATAAGGCCAGAATT
