# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = a* b f* @initial 1.0 M
s1 = e* c* a* @initial 1.0 M
s2 = d* a* d* @initial 1.0 M
s3 = e* f f* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = ACGGATTTTATACGTGA
domains b = CCATCTCACACCAACCT
domains c = ATGTCGCGGAGTGGACG
domains d = GCCTGGTTTGTACAC
domains e = GCTTACGGGATCGTTGA
