# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = a c* d @initial 1.0 M
s1 = b* e* d* @initial 1.0 M
s2 = e* b* a* @initial 1.0 M
s3 = b* d b* @initial 1.0 M

# length
length a = 17
