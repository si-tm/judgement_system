# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a* c* d* @initial 1.0 M
s1 = f* b f @initial 1.0 M
s2 = e* b* e* @initial 1.0 M
s3 = b c* d* @initial 1.0 M
s4 = c* e* d @initial 1.0 M
s5 = f* f a @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CACGAGTTCCGAGCCTC
domains b = GTAGGAATACGGTGTAG
domains c = ATTAAAACATCTTCGTG
domains d = GATCACATGTATTTG
