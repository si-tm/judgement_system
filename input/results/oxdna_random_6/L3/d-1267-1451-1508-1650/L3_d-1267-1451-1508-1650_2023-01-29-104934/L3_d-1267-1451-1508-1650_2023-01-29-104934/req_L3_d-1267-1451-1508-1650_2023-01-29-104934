# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = f c* e @initial 1.0 M
s1 = f a f* @initial 1.0 M
s2 = e e* d* @initial 1.0 M
s3 = f* c* d @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GAAAAGTTTATGCTAAA
domains b = TACCTGCAGGTCAGTTA
domains c = GAGATGTTCTACGGCTG
domains d = CGCGAGTGTACCTAA
domains e = CCTATGGCAAACATGGG
