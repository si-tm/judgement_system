s2 = d c e @initial 1.0 M
s3 = c* e f* @initial 1.0 M
s4 = f* f* a @initial 1.0 M
s5 = a* d e* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

