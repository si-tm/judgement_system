s2 = d* f b* @initial 1.0 M
s3 = b c* b* @initial 1.0 M
s4 = c a d* @initial 1.0 M
s5 = f* a* c* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

