# structure
number_of_types = 4
s0 = b c* b @initial 1.0 M
s1 = a* e e @initial 1.0 M
s2 = d* b c @initial 1.0 M
s3 = f* a* d* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

