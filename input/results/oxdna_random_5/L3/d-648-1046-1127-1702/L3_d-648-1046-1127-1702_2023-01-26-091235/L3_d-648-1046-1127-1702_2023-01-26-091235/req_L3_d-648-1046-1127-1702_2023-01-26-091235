# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = c d a @initial 1.0 M
s1 = d* e* f* @initial 1.0 M
s2 = d* b* b @initial 1.0 M
s3 = f* e* f @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CGTGTTTTTTCCGTCCA
domains b = ACGAGTACCCTCCTGGG
domains c = TTAAAGCCTATCCATGG
domains d = TCCATATCCCCCTGC
domains e = GCAGCGCCGTCACATCG
domains f = ATGGAAACAAGTATAAG
