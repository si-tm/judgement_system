s2 = e* b* b* @initial 1.0 M
s3 = f* f* c @initial 1.0 M
s4 = e d* d @initial 1.0 M
s5 = a* c a* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

