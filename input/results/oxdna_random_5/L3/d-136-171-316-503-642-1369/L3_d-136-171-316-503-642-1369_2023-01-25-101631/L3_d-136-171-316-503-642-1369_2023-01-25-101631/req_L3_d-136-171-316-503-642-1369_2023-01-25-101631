# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a* b b* @initial 1.0 M
s1 = b b c @initial 1.0 M
s2 = b* c* f* @initial 1.0 M
s3 = a f* c @initial 1.0 M
s4 = c c* d @initial 1.0 M
s5 = e* d a* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

