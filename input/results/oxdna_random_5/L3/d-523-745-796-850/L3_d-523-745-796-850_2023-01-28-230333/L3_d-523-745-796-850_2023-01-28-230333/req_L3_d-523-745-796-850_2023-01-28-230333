# structure
number_of_types = 4
s0 = c* d c @initial 1.0 M
s1 = c* f f @initial 1.0 M
s2 = b* d* d* @initial 1.0 M
s3 = c* b a* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CTCCTTGTTGACCACCC
domains b = CCAACAATGACCCAGCG
