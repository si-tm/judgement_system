# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = a d b @initial 1.0 M
s1 = c* a* c* @initial 1.0 M
s2 = d e c @initial 1.0 M
s3 = e* b* c @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CCTTGATAGTCAGATCG
domains b = CTGACGCTGCATTAAAT
domains c = CGCGCGATCAATTGGCG
