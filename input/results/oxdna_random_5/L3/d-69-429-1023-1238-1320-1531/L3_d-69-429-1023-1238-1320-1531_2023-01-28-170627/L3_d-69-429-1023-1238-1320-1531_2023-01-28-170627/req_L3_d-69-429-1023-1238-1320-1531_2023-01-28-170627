# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = f d* d* @initial 1.0 M
s1 = b f* e* @initial 1.0 M
s2 = d* a* b* @initial 1.0 M
s3 = e d* b @initial 1.0 M
s4 = a c* e* @initial 1.0 M
s5 = e* b a @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = CTAATCTACATTGCCTA
domains b = AACAATCTATCACACCG
domains c = CTATGTAATTGCTGTGA
