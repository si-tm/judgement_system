# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c a* b @initial 1.0 M
s1 = c* d* b @initial 1.0 M
s2 = d c b* @initial 1.0 M
s3 = d* e* c @initial 1.0 M
s4 = d f* a* @initial 1.0 M
s5 = c* a* e @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GATATGAACATACAGCA
domains b = AGTCGAGTATGGATTTA
domains c = GACGCCGACTACACGCA
domains d = ACGCCCAGTACAGAC
domains e = ATATTCGAAGTTCCGAC
domains f = TCGTGCGGTGAGTAGCA
