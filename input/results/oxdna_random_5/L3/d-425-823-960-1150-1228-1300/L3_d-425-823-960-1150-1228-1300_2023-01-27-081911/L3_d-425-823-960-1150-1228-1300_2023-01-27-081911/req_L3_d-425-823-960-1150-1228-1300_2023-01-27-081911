# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = e d c @initial 1.0 M
s1 = d e a @initial 1.0 M
s2 = c* e d* @initial 1.0 M
s3 = e* a c @initial 1.0 M
s4 = d* f* f @initial 1.0 M
s5 = b f* c* @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = GTGACATATTGTGAAGT
domains b = CCCACACTTCGCTTGCA
domains c = CGAAATATTACCGAACG
