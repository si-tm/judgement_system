# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = c* f b* @initial 1.0 M
s1 = a a* c* @initial 1.0 M
s2 = f a* a* @initial 1.0 M
s3 = b b f @initial 1.0 M
s4 = a* a* e* @initial 1.0 M
s5 = f* c f @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = TCCGCAACACCGTCGGG
domains b = CGGTACAGTTTTCCAGA
