# type_of_l
type_of_l = L3

# structure
number_of_types = 5
s0 = a a d @initial 1.0 M
s1 = e c a @initial 1.0 M
s2 = e b* b @initial 1.0 M
s3 = d* d e @initial 1.0 M
s4 = c* e e @initial 1.0 M

# length
length a = 17
