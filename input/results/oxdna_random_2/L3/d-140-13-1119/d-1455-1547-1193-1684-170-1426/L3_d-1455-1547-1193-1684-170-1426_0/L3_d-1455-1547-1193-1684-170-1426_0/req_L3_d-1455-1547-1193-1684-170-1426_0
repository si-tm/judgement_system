# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = f a* b* @initial 1.0 M
s1 = f e f* @initial 1.0 M
s2 = e b* c* @initial 1.0 M
s3 = f* e c @initial 1.0 M
s4 = a* b b @initial 1.0 M
s5 = e* f f @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = ATGCATCTCGGTACTAA
domains b = GTTAACGATGTTTGCGT
domains c = AAATCGTTGAACTTTCA
domains d = AGAGTGGCACACACC
domains e = AAACCAGAATAAAGAGG
