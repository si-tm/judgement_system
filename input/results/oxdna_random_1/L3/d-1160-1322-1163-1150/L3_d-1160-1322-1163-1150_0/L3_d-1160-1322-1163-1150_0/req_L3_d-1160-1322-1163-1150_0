# type_of_l
type_of_l = L3

# structure
number_of_types = 4
s0 = e a e @initial 1.0 M
s1 = e* b b @initial 1.0 M
s2 = e a f* @initial 1.0 M
s3 = d* f* f @initial 1.0 M

# length
