# type_of_l
type_of_l = L3

# structure
number_of_types = 6
s0 = a a a @initial 1.0 M
s1 = b* b b @initial 1.0 M
s2 = b* b d @initial 1.0 M
s3 = a c* e @initial 1.0 M
s4 = d e* b* @initial 1.0 M
s5 = a* e* a @initial 1.0 M

# length
length a = 17
length b = 17
length c = 17
length d = 15
length e = 17
length f = 17

# sequence of domains
domains a = TCCAAAGCTCTGAACGC
domains b = TGTACGAGACTGTAGCA
domains c = GGATCGGACCCGAACCG
domains d = TGTTCAGCTCACGAG
domains e = TCTCATTCAGATGTCCT
